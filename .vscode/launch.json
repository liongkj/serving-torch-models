    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Torch Script",
            "type": "python",
            "request": "launch",
            "program": "examples/torchscript/convert.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "examples/flask/app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "1"
            },
            "args": [
                "run"
            ],
        },
    ]
}